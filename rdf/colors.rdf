@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
@prefix col: <http://www.colors.org/> .

col:Red rdf:type owl:Class .
col:Red rdfs:label "Czerwony", "Red" .

col:Green rdf:type owl:Class .
col:Green rdfs:label "Green" .
col:Green rdfs:label "Zielony" .

col:Grassy rdf:type owl:Class .
col:Grassy rdfs:label "Grassy" .
col:Grassy rdfs:subClassOf col:Green .

